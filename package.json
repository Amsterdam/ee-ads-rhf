{
  "name": "@amsterdam/ee-ads-rhf",
  "private": true,
  "license": "EUPL-1.2",
  "engines": {
    "node": ">=22 <25"
  },
  "workspaces": [
    "./packages/*",
    "./storybook"
  ],
  "scripts": {
    "dev": "npm-run-all --parallel lib:dev storybook:start",
    "build": "npm-run-all lib:build storybook:build",
    "test": "npm-run-all lib:test storybook:test",
    "lint": "npm-run-all lib:lint storybook:lint",
    "lib:dev": "npm run dev -w @amsterdam/ee-ads-rhf",
    "lib:build": "npm run build -w @amsterdam/ee-ads-rhf",
    "lib:test": "npm run test -w @amsterdam/ee-ads-rhf",
    "lib:lint": "npm run lint -w @amsterdam/ee-ads-rhf && npm run lint:styles -w @amsterdam/ee-ads-rhf",
    "storybook:start": "npm run start -w @amsterdam/ee-ads-rhf-storybook",
    "storybook:build": "npm run build -w @amsterdam/ee-ads-rhf-storybook",
    "storybook:test": "npm run test -w @amsterdam/ee-ads-rhf-storybook",
    "storybook:lint": "npm run lint -w @amsterdam/ee-ads-rhf-storybook"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "react-select": "^5.10.2",
    "rimraf": "6.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1"
  },
  "dependencies": {
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1"
  }
}
